{
  "name": "interactive-story-starter",
  "description": "A starting point to build your own interactive stories using the Elm Narrative Engine",
  "version": "2.0.1",
  "license": "BSD-3-Clause",
  "author": "Jeff Schomay",
  "repository": {
    "type": "git",
    "url": "https://github.com/jschomay/elm-interactive-story-starter"
  },
  "scripts": {
    "start": "elm-live src/Main.elm --open --start-page=index.html --dir=public -- --output=public/elm.js --debug",
    "build": "rimraf ./dist && copyfiles -f ./public/* ./dist && copyfiles -f ./public/img/* ./dist/img/  && copyfiles -f ./public/styles/* ./dist/styles && elm make src/Main.elm --optimize --output=dist/elm.js",
    "serve-build": "http-server ./dist"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^2.1.0",
    "css-loader": "^0.23.1",
    "elm": "^0.19.0-bugfix2",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^5.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "lodash.assign": "^3.2.0",
    "manifest-loader": "0.0.1",
    "postcss-loader": "^0.9.1",
    "recursive-readdir": "^1.2.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  },
  "dependencies": {
    "elm-live": "^3.4.0",
    "node-gyp": "^3.8.0"
  }
}
